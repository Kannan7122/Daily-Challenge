Code :

#include<stdio.h>
#include<stdlib.h>

int main()
{
    int n,x;
    scanf("%d %d",&n,&x);
    int arr[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d ",&arr[i]);
        if(arr[i]%10==x%10)
        {
            printf("%d ",arr[i]+x);
        }
        else
        {
            printf("%d ",arr[i]-x);
        }
    }
}
