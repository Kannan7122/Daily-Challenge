import java.util.*;
public class Hello {

    public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		String str=sc.next();
		String s="aeiouAEIOU";
		int n=s.length();
		int cnt=0;
		for(int i=0;i<n;i++)
		{
		    if(str.indexOf(s.charAt(i))>=0)
		    {
		        cnt++;
		    }
		}
		if(cnt>=5)
		{
		    System.out.println("yes");
		}
		else
		{
		    System.out.println("no");
		}

	}
}
